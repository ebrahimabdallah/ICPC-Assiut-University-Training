#include<iostream>
#include<algorithm>
#include<string>
 
using namespace std;
 
int main()
{
	long long  n;
	string name;
	cin >> n;
	while (n--)
	{
		cin >> name;
		if (name.find("101")<name.size() || name.find("010")<name.size())
		{
			cout << "Good" << endl;
		}
		else
		{
			cout << "Bad" << endl;
		}
	}
}
